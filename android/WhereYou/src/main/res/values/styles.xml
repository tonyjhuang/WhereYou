<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="WhereYouThemeParent" parent="android:Theme.Holo" />

    <style name="WhereYouTheme">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="MainRowText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/friend_row_height</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/friend_row_text_size</item>
        <item name="typeface">@string/fonttype_medium</item>
    </style>
</resources>